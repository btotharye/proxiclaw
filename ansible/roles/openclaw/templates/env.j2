# OpenClaw Environment Configuration
# Generated by Ansible

# Application Settings
NODE_ENV={{ openclaw_env_vars.NODE_ENV | default('production') }}
PORT={{ openclaw_port }}

# Custom Environment Variables
{% for key, value in openclaw_env_vars.items() %}
{% if key not in ['NODE_ENV'] %}
{{ key }}={{ value }}
{% endif %}
{% endfor %}

